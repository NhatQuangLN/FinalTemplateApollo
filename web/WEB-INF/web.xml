<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/css/*</url-pattern>
        <url-pattern>/js/*</url-pattern>
        <url-pattern>/images/*</url-pattern>
        <url-pattern>/php/*</url-pattern>
        <url-pattern>/scss/*</url-pattern>
        <url-pattern>/webfonts/*</url-pattern>
        <url-pattern>/font/*</url-pattern>
        <url-pattern>/img-test-lastest-post/*</url-pattern>
        <url-pattern>/fontawesome-free-5.15.3-web/*</url-pattern>
        <url-pattern>/MDBootstrap-Swal/css/*</url-pattern>
        <url-pattern>/MDBootstrap-Swal/js/*</url-pattern>
        <url-pattern>/MDBootstrap-Swal/swal-package/dist/*</url-pattern>
             
    </servlet-mapping>    
    <welcome-file-list>
        <!--<welcome-file>home.jsp</welcome-file>-->
        <welcome-file>AddPost.jsp</welcome-file>
             
    </welcome-file-list>
</web-app>
